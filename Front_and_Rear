// You are using GCC
#include<iostream>
#include<queue>

using namespace std;

int main(){
    int num;
    cin >> num;
    queue<int> q;
    for(int i=0;i<num;i++){
        int n;
        cin >> n;
        q.push(n);
    }
    cout << "Front element : " << q.front() << endl;
    cout << "Rear element : " << q.back();
    
    return 0;
}
